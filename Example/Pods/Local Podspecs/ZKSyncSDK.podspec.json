{
  "name": "ZKSyncSDK",
  "version": "0.1.0",
  "summary": "A short description of ZKSyncSDK.",
  "description": "zkSync is a scaling and privacy engine for Ethereum. Its current functionality scope includes low gas transfers of ETH and ERC20 tokens in the Ethereum network",
  "homepage": "https://github.com/zksync-sdk/zksync-sdk-swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "The Matter Labs team": "hello@matterlabs.dev"
  },
  "platforms": {
    "ios": "9.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/zksync-sdk/zksync-sdk-swift.git",
    "tag": "0.1.0"
  },
  "module_map": "ZKSyncSDK/ZKSyncSDK.modulemap",
  "vendored_libraries": "ZKSyncSDK/libzks/*.a",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "ZKSyncSDK/Classes/**/*",
        "ZKSyncSDK/Headers/*.h"
      ],
      "dependencies": {
        "BigInt": [

        ],
        "ZKSyncSDK/Crypto": [

        ],
        "Alamofire": [

        ],
        "web3swift": [

        ]
      }
    },
    {
      "name": "Crypto",
      "source_files": "ZKCryptoSDK/Classes/**/*",
      "pod_target_xcconfig": {
        "VALID_ARCHS": "arm64 arm64e armv7 armv7s x86_64"
      }
    }
  ],
  "swift_version": "5.0"
}
